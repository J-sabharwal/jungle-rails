<tr>
  <td>
  <%= image_tag lineItem.product.image.tiny %>
  </td>
  <td>
    <h4><%= lineItem.product.name %></h4>
    <br>
    <p><%= lineItem.product.description %></p>
  </td>
  <td>
    <%= number_to_currency lineItem.product.price %>
  </td>
  <td>
    
    <%= lineItem.quantity %>
    
  </td>
  <td>
    <%= number_to_currency lineItem.product.price * lineItem.quantity %>
  </td>
</tr>



